$('#ticketsloop').append('<%= escape_javascript(render("tickets")) %>');

 <% if @tickets.current_page < @tickets.total_pages %>
		 $('div.pagination_ticket a.buttom_twitter').attr('href','http://localhost:3000/landing/ticket?page=<%= @tickets.current_page + 1 %>');
 		 $('div#carregandot').hide();
 <% end %>
 
 <% if @tickets.total_pages == @tickets.current_page %>
     $('div.pagination_ticket').remove();
     $('div#carregandot').hide();
 <% end %>