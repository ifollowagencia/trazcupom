$('#ticketsloop').append('<%= escape_javascript(render("tickets")) %>');

<% if @tickets.current_page < @tickets.total_pages %>
	$('.buttom_twitter').attr('href','ticket?page=<%= @tickets.current_page + 1 %>');
	$('.pagination_ticket').show();
	$('#carregandot').hide();
<% end %>

<% if @tickets.total_pages == @tickets.current_page %>
    $('.pagination_ticket').remove();
    $('.paginationt').hide();
<% end %>




