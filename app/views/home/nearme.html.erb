
<!-- event -->
<section id="events" style="background:black; ">
	<div class="container">
<!-- section header -->		
		<div class="header">
			<h2>Cupons Recentes</h2>
			<p>Seja bem vindo(a) <%= @name %>, navegue por todo o aplicativo conhecendo novos estabelecimentos e aproveitando cupons de desconto gratuitos!</p>
		</div>
<!-- section header end -->
<!-- event blocks -->
		<div class="row">


		<% @cupons.each do |cupom| %>

			<div class="span4 eblock">
				<div class="econtainer">
					<div class="ehalf">
						<%= image_tag cupom.image1_url(:banner_thumb) %>
					</div>
					<div class="ehalf">
						<h3><a href="#"><%= cupom.establishment.reason %></a></h3>
						<i class="icon-map-marker"><span><%= cupom.establishment.adress %></span></i>
						<br />
						<i class="icon-home"><span><%= cupom.establishment.city.name %></span></i><br />
						<i class="icon-time"><span><%= cupom.validity.strftime('%d/%m/%y') %></span></i><br />
						<div class="date">
							<span class="day"></span>
							<span class="week">pegar</span>
						</div>						
					</div>
				</div>
			</div>

			<% end %>
	
		</div>
<!-- event blocks end -->

	</div>
</section>
<!-- event end -->

<!-- Audios -->
<section id="audio" class="audios" style="background:#6b809b;">
	<div class="container">
<!-- section header -->		
		<div class="header">
			<h2>Estabelecimentos</h2>
			<p>Aqui você poderá conhecer os estabelecimentos e acompanhar os seus produtos.</p>
		</div>
<!-- section header end -->
<!-- tracks -->
	<div class="row">
		<div id="sm2-container">
			<!-- sm2 flash goes here -->
		</div>	
		<div class="row"> 

		<% @estabs.each do |estab|%>
			<div class="track">
				<div class="sm2-visualization">	
					<div class="teste" style="background:#fff;">
						<%= link_to(image_tag(estab.imageprofile_url(:banner_thumb)), estab) %>
					</div>
				</div>
				<div class="up"><i class="icon-caret-up"></i></div>

				<div class="tinfo">
					<h3><%= estab.reason %></h3>

						<ul class="social">
							<li><a href="#"><strong>Compartilhe com seus amigos:</strong></a></li>
							<li><a href="#" data-placement="top" data-toggle="tooltip" title="facebook"><i class="icon-facebook"></i></a></li>								
							<li><a href="#" data-placement="top" data-toggle="tooltip" title="twitter"><i class="icon-twitter"></i></a></li>
							<li><a href="#" data-placement="top" data-toggle="tooltip" title="google plus"><i class="icon-google-plus"></i></a></li>					
							<li><a href="#" data-placement="top" data-toggle="tooltip" title="tumblr"><i class="icon-tumblr"></i></a></li>							
						</ul>	
				</div>
			</div>
			<% end %>

	

	   	</div>
	</div>
<!-- tracks end -->
	</div>
</section>
<!-- Audios end -->



