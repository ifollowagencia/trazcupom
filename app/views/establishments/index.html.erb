<h1>Listing establishments</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Surname</th>
      <th>Phone</th>
      <th>Email</th>
      <th>Password</th>
      <th>Cnpj</th>
      <th>Reason</th>
      <th>City</th>
      <th>Plan</th>
      <th>Imageprofile</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @establishments.each do |establishment| %>
      <tr>
        <td><%= establishment.name %></td>
        <td><%= establishment.surname %></td>
        <td><%= establishment.phone %></td>
        <td><%= establishment.email %></td>
        <td><%= establishment.password %></td>
        <td><%= establishment.cnpj %></td>
        <td><%= establishment.reason %></td>
        <td><%= establishment.city.name %></td>
        <td><%= establishment.plan %></td>
        <td><%= establishment.imageprofile %></td>
        <td><%= link_to 'Show', establishment %></td>
        <td><%= link_to 'Edit', edit_establishment_path(establishment) %></td>
        <td><%= link_to 'Destroy', establishment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Establishment', new_establishment_path %>
