    <%= f.label :latitude %><br>
    <%= f.text_field :latitude %>
  </div>
  <div class="field">
    <%= f.label :longitude %><br>
    <%= f.text_field :longitude %>
  </div>
  <div class="field">
    <%= f.label :establishment %><br>
    <%= collection_select(:address_establishment, :establishment_id, Establishment.all, :id, :reason, prompt: 'selecione o estabelecimento') %>
