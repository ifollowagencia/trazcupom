<%= form_for(@address_establishment) do |f| %>
  <% if @address_establishment.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@address_establishment.errors.count, "error") %> prohibited this address_establishment from being saved:</h2>

      <ul>
      <% @address_establishment.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :address %><br>
    <%= f.text_field :address %>
  </div>
