<h1>Listing categoryestablishments</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @categoryestablishments.each do |categoryestablishment| %>
      <tr>
        <td><%= categoryestablishment.name %></td>
        <td><%= link_to 'Show', categoryestablishment %></td>
        <td><%= link_to 'Edit', edit_categoryestablishment_path(categoryestablishment) %></td>
        <td><%= link_to 'Destroy', categoryestablishment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

