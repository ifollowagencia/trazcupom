<body>
<!-- header -->	
<header id="mt-header" class="header-bg-style">
		<div class="container">
			<div class="logo">
				<a href="#"><img src="/home/images/logo01.png" alt="logo"/></a>
			</div>
			<div class="hgroup">
				<h1></h1>
				<h2>Encontre os melhores cupons <br>de sua cidade <span><strong style="color:white;">e obtenha vantagens exclusivas</strong></span>.</h2>
			</div>
		</div> 
</header>
<!-- header end -->

<!-- nav -->
<nav>
	<div class="main-nav">

            <div class="container">
	            <button data-target=".nav-collapse" data-toggle="collapse" class="mobilenav" type="button">
	                  <span class="title">Menu</span><span class="trig icon-align-justify"></span>
	            </button>
	            <!-- nav menu -->
	            <div class="nav-collapse collapse">
			        <ul class="nav">
			            <li><%= link_to('Perfil', :controller => 'user', :action => 'profile') %></li>
						<li><%= link_to('Configurações', :controller => 'user', :action => 'profile') %></li>
						<li><%= link_to('Meus Cupons', :controller => 'user', :action => 'profile') %></li>

						<li><%= link_to('Voltar ao app', :controller => 'home', :action => 'nearme') %></li>
						<li><%= link_to('Sair', destroy_user_session_path, :method => :delete) %></li>

						
					
			        </ul>
			        
	   			</div>
				<!-- nav menu end-->
			
			        

            </div>
	</div>	

			  
</nav>
<!-- nav end -->
<%= yield %>
<!-- Contact end -->
<footer style="background:black;">
	<div class="container">
		<div class="copyright">Traz Cupom <p>&copy;2013 Todos os direitos reservados</p></div>
		<div class="info">
			<ul>
	<li><a href="http://www.ifollowagencia.com.br"><img src="/home/images/logo_primary.png" width="140" height="70" /></a></li>
			</ul>

		</div>
	</div>
</footer>
<!--JavaScript-->
<script type="text/javascript" src="/home/js/jquery-1.8.1.min.js"></script>
